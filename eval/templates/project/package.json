{
	"name": "@storybook/mcp-eval-template-project",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"eval:build": "vite build",
		"eval:build-storybook": "storybook build",
		"eval:dev": "vite",
		"eval:lint": "eslint .",
		"preview": "vite preview",
		"eval:test": "vitest run --reporter json --outputFile ../results/tests.json",
		"eval:test:coverage": "pnpm run eval:test --coverage",
		"eval:typecheck": "tsc --noEmit --project ./tsconfig.app.json"
	},
	"dependencies": {
		"react": "catalog:tasks",
		"react-dom": "catalog:tasks"
	},
	"devDependencies": {
		"@eslint/js": "catalog:tasks",
		"@types/node": "catalog:tasks",
		"@types/react": "catalog:tasks",
		"@types/react-dom": "catalog:tasks",
		"@vitejs/plugin-react-swc": "catalog:tasks",
		"eslint": "catalog:tasks",
		"eslint-plugin-react-hooks": "catalog:tasks",
		"eslint-plugin-react-refresh": "catalog:tasks",
		"globals": "catalog:tasks",
		"typescript": "catalog:tasks",
		"typescript-eslint": "catalog:tasks",
		"vite": "catalog:tasks"
	}
}
